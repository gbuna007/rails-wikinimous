<h1>New Article</h1>
<%= form_for(@article) do |f| %>
  <%= f.label :title, class: "form-label" %>
  <%= f.text_field :title, class: "form-control" %>
  <%= f.label :content, class: "form-label" %>
  <%= f.text_area :content, size: "70x5", class: "form-control"  %>
  <br><%= f.submit%>
<% end %>
<br>
<%= link_to 'Back to list', root_path %>
